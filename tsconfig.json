{
  "compilerOptions": {
    "target": "ES2017",
    "module": "CommonJS",
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "dist",
    "sourceMap": true,
    "incremental": true,
    "declaration": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "removeComments": true,
    "strictNullChecks": false,
    "strictBindCallApply": false,
    "strictPropertyInitialization": false,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "noImplicitThis": false,
    "allowJs": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "@src/*": [
        "src/*"
      ],
      "@src": [
        "src/index"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@components": [
        "src/components/index"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@styles": [
        "src/styles/index"
      ],
      "@console/*": [
        "src/console/*"
      ],
      "@console": [
        "src/console/index"
      ]
    }
  }
}